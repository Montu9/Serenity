{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "mailing/templates/**/*",
        "outDir": "dist/src",
        "watchAssets": true
      },

      {
        "include": "secrets",
        "outDir": "dist",
        "watchAssets": true
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true
  },
  "entryFile": "/src/main.js"
}
